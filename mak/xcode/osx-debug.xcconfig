ARCHS = $(ARCHS_STANDARD_32_64_BIT)
SDKROOT = macosx10.5
GCC_PREPROCESSOR_DEFINITIONS_PLATFORM[arch=i386] = BE_STATIC _DARWIN _X86 BE_PLATFORM="platform_darwin"
GCC_PREPROCESSOR_DEFINITIONS_PLATFORM[arch=x86_64] = BE_STATIC _DARWIN _AMD64 BE_PLATFORM="platform_darwin"
GCC_PREPROCESSOR_DEFINITIONS_PLATFORM[arch=ppc] = BE_STATIC _DARWIN BE_BIGENDIAN _POWERPC _PPC BE_PLATFORM="platform_darwin"
GCC_PREPROCESSOR_DEFINITIONS_PLATFORM[arch=ppc64] = BE_STATIC _DARWIN BE_BIGENDIAN _POWERPC _PPC _PPC64 BE_PLATFORM="platform_darwin"
DEBUG_INFORMATION_FORMAT = dwarf-with-dsym
USE_HEADERMAP = NO
