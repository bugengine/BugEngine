load plugin.graphics.text as text;
load plugin.graphics.shadermodel1 as sm1;

font = text.OutlineFont
(
	fontFile = <DroidSans.ttf>;
)

shader = text.OutlineShader
(
    textColor = sm1.Float.Float4.Constant
	(
		value = <1.0f, 1.0f, 1.0f, 1.0f>;
	);
    backgroundColor = sm1.Float.Float4.Constant
	(
		value = <1.0f, 1.0f, 1.0f, 1.0f>;
	);
)

