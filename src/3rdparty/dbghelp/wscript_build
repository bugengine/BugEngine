if not m.has_key(name):
    exparchoptions = {
        'win32-x86': coptions(libs=['psapi','dbghelp','version'], libdir=[os.path.join('src', '3rdparty', name, 'lib.x86')]),
        'win32-amd64': coptions(libs=['psapi','dbghelp','version'], libdir=[os.path.join('src', '3rdparty', name, 'lib.amd64')]),
        }
    m[name] = util( name, [], '3rdparty', coptions(), coptions(), {}, exparchoptions)
